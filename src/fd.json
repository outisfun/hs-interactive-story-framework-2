{
    "ELEMENTS": {
        "GALLERY": {
            "CLASSES": {
                "EL": "js--gallery",
                "INNER": "js--gallery__inner",
                "ITEM": "js--gallery__item"
            },
            "CONSTRUCTOR": "ISF_Element_Gallery",
            "DATA": {
                "galleryLayout": {
                    "DATA_TYPEOF": "string",
                    "DATA_VALUES": [
                        "masonry",
                        "grid",
                        "preview"
                    ],
                    "DATA_USAGE": null
                },
                "galleryPreview": {
                    "DATA_TYPEOF": "boolean"
                },
                "galleryImages": {
                    "DATA_TYPEOF": "objectsArray",
                    "DATA_VALUES": {
                        "img": "string",
                        "src": "string"
                    }
                }
            }
        }
    },
    "LAYOUTS": {
        "SIMPLE": {
            "CLASSES": {
                "EL": "js--layout_simple",
                "CONTENT": "js--content"
            },
            "DATA": {
                "colorScheme": {
                    "DATA_TYPEOF": "string",
                    "DATA_VALUES": [
                        "dark",
                        null
                    ]
                },
                "container": {
                    "DATA_TYPEOF": "string",
                    "DATA_VALUES": [
                        "container",
                        "container-fluid"
                    ]
                },
                "lovers": {
                    "DATA_TYPEOF": "objectsArray",
                    "DATA_VALUES": {
                        "img": "string",
                        "src": "string"
                    }
                }
            }
        },
        "CHAPTER": {
            "CLASSES": {
                "EL": "js--chapter",
                "CONTENT": "js--chapter__content",
                "HEADER": "js--chapter__header"
            },
            "CONSTRUCTOR": "ISF_Layout_Chapter",
            "DATA": {
                "chapterStyle": {
                    "DATA_TYPEOF": "string",
                    "DATA_VALUES": [
                        "ralph-lauren",
                        "ellie",
                        "mercedes",
                        "aleister"
                    ],
                    "DATA_USAGE": {
                        "MANDATORY": true
                    }
                },
                "chapterBackground": {
                    "DATA_TYPEOF": "imgsrc",
                    "MANDATORY": true
                },
                "chapterTitle": {
                    "DATA_TYPEOF": "string",
                    "MANDATORY": true
                }
            }
        }
    }
}